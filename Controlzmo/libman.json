{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jqueryui@1.12.1",
      "destination": "wwwroot/js/jqueryui/",
      "files": [
        "jquery-ui.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/js/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.js.map"
      ]
    },
    {
      "library": "nosleep@0.12.0",
      "destination": "wwwroot/js/nosleep/",
      "files": [
        "NoSleep.js"
      ]
    }
  ]
}