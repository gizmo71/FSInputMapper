<Window x:Class="FSInputMapper.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FSInputMapper"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox HorizontalAlignment="Left" Margin="120,87,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Text="{Binding Text}" TextChanged="TestTextBox_TextChanged"/>
        <Button Content="Push" HorizontalAlignment="Left" Margin="259,86,0,0" VerticalAlignment="Top" Click="Push_Clicked"/>
        <Button Content="Pull" HorizontalAlignment="Left" Margin="316,86,0,0" VerticalAlignment="Top" Click="Pull_Clicked"/>
    </Grid>
</Window>
